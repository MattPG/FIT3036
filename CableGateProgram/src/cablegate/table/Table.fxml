<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <TextField fx:id="searchField" prefHeight="25.0" prefWidth="217.0" text="Enter search words here!">
                           <HBox.margin>
                              <Insets left="15.0" right="5.0" />
                           </HBox.margin>
                        </TextField>
                        <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="264.0">
                     <children>
                        <Button fx:id="exploreButton" mnemonicParsing="false" text="Explore Selected Cable..." />
                     </children>
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </HBox>
               </children>
            </HBox>
            <StackPane>
               <children>
                  <TableView fx:id="tablePane" />
               </children>
            </StackPane>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <Button fx:id="prevButton" mnemonicParsing="false" text="Prev." />
                  <Label fx:id="pageNumLabel" text="Page Num Label" />
                  <Button fx:id="nextButton" mnemonicParsing="false" text="Next" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="50.0" right="50.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
